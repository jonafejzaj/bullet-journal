<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<Scene xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <AnchorPane prefHeight="158.0" prefWidth="360.0">
      <children>
         <VBox layoutX="-1.0" layoutY="1.0" prefHeight="166.0" prefWidth="360.0" spacing="10.0">
            <children>
               <Label text="Choose your &quot;.bujo&quot; file">
                  <font>
                     <Font name="Andale Mono" size="13.0" />
                  </font>
               </Label>
               <Button fx:id="chooseFile" mnemonicParsing="false" prefHeight="25.0" prefWidth="409.0" text="Choose File" />
               <Label text="Create New &quot;.bujo&quot; file">
                  <font>
                     <Font name="Andale Mono" size="13.0" />
                  </font>
               </Label>
               <Button fx:id="newFile" mnemonicParsing="false" prefHeight="27.0" prefWidth="389.0" text="New File">
                  <font>
                     <Font name="Andale Mono" size="13.0" />
                  </font>
               </Button>
            </children>
            <padding>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
         </VBox>
      </children></AnchorPane>
</Scene>
