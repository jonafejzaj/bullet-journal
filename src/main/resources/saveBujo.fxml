<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<Scene xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <root>
      <BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="151.0" prefWidth="555.0">
         <center>
            <VBox alignment="CENTER" prefHeight="272.0" prefWidth="520.0" spacing="10.0" BorderPane.alignment="CENTER">
               <children>
                  <Label text="Select a file to save your &quot;.bujo&quot; file:">
                     <font>
                        <Font name="Andale Mono" size="13.0" />
                     </font>
                  </Label>
                  <HBox alignment="CENTER" prefHeight="31.0" prefWidth="419.0" spacing="10.0">
                     <children>
                        <TextField fx:id="saveFileTxt" alignment="CENTER" prefHeight="25.0" prefWidth="397.0" />
                     </children>
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                     <VBox.margin>
                        <Insets left="20.0" />
                     </VBox.margin>
                  </HBox>
                  <ButtonBar prefHeight="40.0" prefWidth="200.0">
                     <buttons>
                        <Button fx:id="saveOk" alignment="CENTER" mnemonicParsing="false" text="OK" />
                        <Button fx:id="saveCancel" alignment="CENTER" mnemonicParsing="false" text="CANCEL" />
                     </buttons>
                  </ButtonBar>
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </VBox>
         </center>
      </BorderPane>
   </root>
</Scene>
